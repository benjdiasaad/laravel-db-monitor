{
   "name": "benjdiasaad/laravel-db-monitor",
    "description": "Real-time database health monitoring for Laravel applications. Detects slow queries, N+1 issues, missing indexes, and suggests performance optimizations.",
    "keywords": [
        "laravel",
        "laravel-package",
        "database",
        "database-monitoring",
        "performance",
        "performance-optimization",
        "slow-queries",
        "n-plus-one",
        "query-analysis",
        "missing-index",
        "sql-optimization",
        "developer-tools",
        "backend-tools"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Benjdiasaad\\DbMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benjdiasaad\\DbMonitor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["BenjdiaSaad\\DbMonitor\\DbMonitorServiceProvider"],
            "aliases": {
                "DbMonitor": "BenjdiaSaad\\DbMonitor\\Facades\\DbMonitor"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
