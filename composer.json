{
    "name": "benjdiasaad/laravel-db-monitor",
    "description": "Real-time database health monitoring for Laravel: slow queries, N+1 detection, and missing index suggestions.",
    "keywords": ["laravel", "database", "monitoring", "query", "slow-query", "performance"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Benjdiasaad\\DbMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benjdiasaad\\DbMonitor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["BenjdiaSaad\\DbMonitor\\DbMonitorServiceProvider"],
            "aliases": {
                "DbMonitor": "BenjdiaSaad\\DbMonitor\\Facades\\DbMonitor"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
